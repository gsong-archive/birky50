{
  "name": "birky50",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules/bootstrap/scss src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules/bootstrap/scss src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom --modulePaths=src",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.1",
    "emotion": "^9.1.3",
    "eslint": "^4.19.1",
    "facepaint": "^1.2.1",
    "node-sass-chokidar": "^1.1.2",
    "npm-run-all": "^4.1.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-emotion": "^9.1.3",
    "react-scripts": "^1.1.1",
    "reactstrap": "~6.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "cypress": "^2.1.0",
    "dotenv": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "faker": "^4.1.0",
    "jest-emotion": "^9.1.3",
    "prettier": "^1.11.1",
    "puppeteer": "^1.4.0",
    "react-testing-library": "~3.0.2",
    "snapshot-diff": "^0.3.0"
  }
}
